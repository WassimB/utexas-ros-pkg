<launch>
  <node name="camera_person_detector" pkg="camera_person_detector" type="detector">
    <param name="use_hog_descriptor" value="false" />
    <param name="use_haar_cascade" value="true" />
    <param name="haar_cascade_file" value="$(find camera_person_detector)/haarcascades/haarcascade_upperbody.xml" />
    <param name="image_transport" value="compressed" />
    <remap from="image_raw" to="camera1/image_raw" />
  </node>
</launch>
